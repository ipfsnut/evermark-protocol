{
  "name": "@ipfsnut/evermark-metadata-kit",
  "version": "1.0.0",
  "description": "Content extraction and metadata processing SDK for Evermark Protocol",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "clean": "rm -rf dist",
    "test": "vitest",
    "lint": "eslint src/**/*.ts",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@ardrive/turbo-sdk": "^1.31.1",
    "viem": "^2.21.0",
    "sharp": "^0.34.0"
  },
  "devDependencies": {
    "@types/node": "^22.0.0",
    "typescript": "^5.6.0",
    "vitest": "^2.1.0",
    "eslint": "^9.0.0"
  },
  "keywords": [
    "metadata",
    "content-extraction",
    "doi",
    "isbn",
    "farcaster",
    "twitter",
    "arweave",
    "ipfs"
